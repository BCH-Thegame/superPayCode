.f-c{
    display: flex;
    align-items:center;
    justify-content:space-between;
}
.bline {
    position: relative;
}
.bline:before {
    content: '';
    position: absolute;
    left: -50%;
    right: -50%;
    bottom: 0;
    border-bottom: 1px solid #EAEBEC;
    transform: scale(0.5);
}

.list-arr::after{
     content: '';
    display:block;
    width:18rpx;
    height:18rpx;
    border:2px solid #D5D6D9;
    border-left:none;
    border-bottom:none;
    transform:rotate(45deg);
    flex-basis:18rpx;
}

