.m-list>view{
    padding:38rpx 40rpx;
    display:flex;
    align-items:center;
    justify-content:space-between;
    font-size:30rpx;
    color:#5A5D67;
}
.m-list-value{
    flex-grow:2;
    text-align:right;
    margin-right:10rpx;
    color:#5A5D67;
    font-size:32rpx;
    font-weight:normal;
}
.m-list>view::after{
     content: '';
    display:block;
    width:18rpx;
    height:18rpx;
    border:2px solid #eee;
    border-left:none;
    border-bottom:none;
    transform:rotate(45deg);
  
}
.m-head{
    text-align:center;
    display:flex;
    flex-direction:column;
    align-items:center;
    justify-content:center;
    padding:90rpx 110rpx;
    background:#313541;
}
.m-img{
    width:140rpx;
    height:140rpx;
    border-radius:200px;
    overflow:hidden;
}
.m-name{
    color:#fff;
}
.m-list .ico{
    font-size:44rpx;
    margin-right:18rpx;
}
.m-price{
    color:#24B53E;
    font-weight:800;
}
.m-price text{
    font-size:25rpx;
}